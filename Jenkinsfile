pipeline{
    agent any
    stages{
        stage('Checkout code'){
            steps{
                sh ''' echo 'This is first step11' '''
            }
        }
        stage('Build'){
            steps{
                sh ''' echo 'This is first step21' '''
            }
        }
        stage('Deploy to dev'){
            steps{
                sh ''' echo 'This is first step31' '''
            }
        }
    }
}